      <Button fx:id="btnSupprLieu" layoutX="629.0" layoutY="528.0" mnemonicParsing="false" onAction="#supprimerLieuClick" text="Supprimer un lieu" />
      <Label layoutX="35.0" layoutY="22.0" text="Les Lieux" />
